    "configuration": {
      "title": "VS Code Ribbon",
      "properties": {
        "vscodeRibbon.layout": {
          "type": "object",
          "default": {
            "version": 1,
            "tabs": []
          },
          "markdownDescription": "Ribbon layout definition (tabs, groups, items).",
          "additionalProperties": true
        },
        "vscodeRibbon.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable ribbon UI actions and slot command execution."
        },
        "vscodeRibbon.writeTarget": {
          "type": "string",
          "enum": [
            "user",
            "workspace"
          ],
          "default": "workspace",
          "description": "Where layout changes are persisted."
        },
        "vscodeRibbon.showLabels": {
          "type": "boolean",
          "default": true,
          "description": "Show item labels under icons."
        },
        "vscodeRibbon.iconSize": {
          "type": "string",
          "enum": [
            "small",
            "medium",
            "large"
          ],
          "default": "medium",
          "description": "Icon size in the ribbon."
        },
        "vscodeRibbon.compactMode": {
          "type": "boolean",
          "default": false,
          "description": "Use compact spacing in the ribbon UI."
        },
        "vscodeRibbon.slotMinWidth": {
          "type": "number",
          "default": 112,
          "minimum": 72,
          "maximum": 400,
          "description": "Minimum width in pixels for a ribbon slot card when variable-width layout is used."
        },
        "vscodeRibbon.slotMaxWidth": {
          "type": "number",
          "default": 280,
          "minimum": 140,
          "maximum": 640,
          "description": "Maximum width in pixels for a ribbon slot card when variable-width layout is used."
        },
        "vscodeRibbon.slotShowOutline": {
          "type": "boolean",
          "default": false,
          "description": "Show themed border/background outlines for ribbon slot cards."
        },
        "vscodeRibbon.autoAcceptCommandPaletteSelection": {
          "type": "boolean",
          "default": true,
          "description": "When a ribbon item runs command-palette quickOpen queries, auto-accept the selected entry so Enter is not required."
        },
        "vscodeRibbon.capturePaletteWrapperCommands": {
          "type": "boolean",
          "default": false,
          "description": "Capture quick input wrapper commands (quickOpen/showCommands) when assigning from Command Palette. Disable to prefer direct executable command IDs."
        }
      }
    }
